    <GeneDef ParentName="GeneBloodColorBase">
        <defName>GEB_Blood_Yellowfin</defName>
        <label>Yellowfin Blood</label>
        <description>This gene triggers a biochemical reaction in the creature, causing its blood to turn Yellow.</description>
        <iconPath>UI/Icons/Genes/Yellowfin_Icon</iconPath>
        <displayOrderInCategory>0</displayOrderInCategory>
        <modExtensions>
            <li Class="VanillaGenesExpanded.GeneExtension">
                <customBloodThingDef>GEW_Blood_Filth_Yellowfin</customBloodThingDef>
                <customBloodIcon>UI/Icons/Medical/Yellowfin_BleedingIcon</customBloodIcon>
                <customBloodEffect>GEB_Damage_Yellowfin</customBloodEffect>
                <customWoundsFromFleshtype>GEB_FleshType_Yellowfin</customWoundsFromFleshtype>
            </li>
        </modExtensions>
    </GeneDef>

